{
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "live": "next build && next start",
    "test": "yarn prettier --check . && yarn eslint './**/*.{ts,tsx}' && yarn tsc",
    "fix": "yarn prettier --write --list-different . && yarn eslint './**/*.{ts,tsx}' --fix",
    "deploy:prepare:out": "next build && next export",
    "deploy:prepare:stripped": "mkdir stripped/out -p && find out -type f -name '*.html' | while read f; do cp \"$f\" \"stripped/${f%.html}\"; done",
    "deploy:prepare": "yarn deploy:prepare:out && yarn deploy:prepare:stripped",
    "deploy:upload:out": "aws s3 sync out s3://jamiebray-ui --delete",
    "deploy:upload:stripped": "aws s3 sync stripped/out s3://jamiebray-ui --content-type \"text/html\"",
    "deploy:upload": "yarn -s deploy:upload:out && yarn -s deploy:upload:stripped",
    "deploy": "yarn deploy:prepare && yarn deploy:upload"
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/next": "^9.0.0",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.34",
    "@types/react-transition-group": "^4.2.4",
    "@types/styled-components": "^5.1.0",
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "eslint-config-prettier": "^6.11.0",
    "lorem-ipsum": "^2.0.3",
    "next": "^9.3.5",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.4.1",
    "node-sass": "^4.13.1",
    "now": "^20.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.1.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^6.0.20",
    "@storybook/react": "^6.0.20",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "^2.1.1"
  }
}
